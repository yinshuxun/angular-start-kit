{
  "name": "angular-start-kit",
  "version": "1.0.0",
  "description": "just do it",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon --exec \"npm run webpack-dev-server -- --config ./entry.js --open --progress --profile --watch --content-base src/\"",
    "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js",
    "build": "npm run build:dev",
    "build:dev": "npm run clean:dist && npm run webpack -- --config ./entry.js --progress --profile",
    "build:prod": "npm run clean:dist && npm run webpack -- --config ./entry.js  --progress --profile --bail",
    "clean:dll": "npm run rimraf -- dll",
    "clean:aot": "npm run rimraf -- compiled",
    "clean:dist": "npm run rimraf -- dist",
    "rimraf": "rimraf",
    "clean:install": "npm set progress=false && npm install"
  },
  "keywords": [
    "angular",
    "webpack",
    "tslint"
  ],
